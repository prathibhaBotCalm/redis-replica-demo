{
  "required_status_checks": {
    "strict": true,
    "contexts": [
      "Code Quality Check",
      "Linting Check",
      "Unit Tests",
      "Build Check",
      "Security Check"
    ]
  },
  "enforce_admins": true,
  "required_pull_request_reviews": {
    "dismissal_restrictions": {
      "users": ["lead-dev", "tech-lead"],
      "teams": ["reviewers"]
    },
    "require_code_owner_reviews": true,
    "required_approving_review_count": 2
  },
  "required_conversation_resolution": true,
  "restrictions": {
    "users": ["lead-dev"],
    "teams": ["dev-team"]
  }
}
